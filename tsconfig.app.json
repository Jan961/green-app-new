{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "public",
    "src/assets/**",
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "skipLibCheck": true,
    "types": [],
    "paths": {
      "@/*": ["./src/*"]
    },
  }
  ,
  "watchOptions": {
    "excludeDirectories": [
      "**/node_modules/**",
      "**/.git/**",
      "**/dist/**",
      "**/public/**",
      "**/src/assets/**"
    ]
  }
}
